{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "import-name": [
            false,
            {
                "react": "React",
                "react-dom": "ReactDOM",
                "react-helmet": "Helmet"
            }
        ],
        "variable-name": [
            true,
            "allow-pascal-case"
        ]
    }
}